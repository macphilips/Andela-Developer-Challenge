{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint server/src/**/*.js ui/src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:ui": "eslint client/src/**/*.{js,jsx}",
    "clean:server": "rimraf server/build/",
    "clean:ui": "rimraf client/dist/",
    "build-babel": "babel server/src -d server/build/src",
    "build": "npm run clean:server && npm run build-babel",
    "start": "npm run build && node --require dotenv/config ./server/bin/server",
    "test": "npm run test:server && npm run test:ui",
    "test:server": "cross-env NODE_ENV=test nyc mocha \"server/src/test/**/*.js\"",
    "test:ui": "jest --coverage",
    "webpack:dev": "npm run clean:ui && webpack --config webpack/webpack.dev.js --profile --env.PLATFORM=local --env.VERSION=stag",
    "webpack:prod": "npm run clean:ui && webpack --config webpack/webpack.prod.js --profile",
    "coverage": "npm run coveralls:coverage && npm run sonar:coverage",
    "coveralls:coverage": "nyc report --reporter=text-lcov | coveralls",
    "sonar:coverage": "cross-var sonar-scanner -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$SONAR_CLOUD_TOKEN",
    "webpack": "webpack-dev-server --config webpack/webpack.dev.js --mode development --inline --hot --profile --port=9060 --watch-content-base"
  },
  "dependencies": {
    "ant-pattern-matcher": "0.0.4",
    "bcryptjs": "^2.4.3",
    "bluebird": "~3.5.1",
    "chai": "^3.5.0",
    "chai-http": "^4.0.0",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "firebase": "^5.4.0",
    "firebase-admin": "^6.0.0",
    "has": "^1.0.3",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "~1.9.0",
    "node-schedule": "^1.3.0",
    "pg-promise": "^8.4.5",
    "prop-types": "^15.6.2",
    "randomstring": "^1.1.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-mock-router": "^1.0.15",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-router-prop-types": "^1.0.4",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.0.5",
    "react-select": "^2.0.0-beta.7",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "sinon": "^7.1.1",
    "swagger-ui-dist": "^3.17.6",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-airbnb": "^3.0.1",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cross-var": "^1.1.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-jest-enzyme": "^6.0.2",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "faker": "^4.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "jest-fetch-mock": "^1.7.4",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.8.3",
    "nyc": "^12.0.2",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "react-test-renderer": "^16.6.3",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "nyc": {
    "require": [
      "@babel/register",
      "dotenv/config"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
